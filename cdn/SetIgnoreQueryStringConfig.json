  },
  "parameters": [
    {
      "name": "AccessKeyId",
      "position": "Query",
      "type": "String",
      "description": {
        "zh": "当认证方式为Accesskey时必填，用户需要显式指定，校验签名使用，不指定则不校验签名"
      },
      "required": false,
      "hidden": false
    },
    {
      "name": "Action",
      "position": "Query",
      "type": "String",
      "description": {
        "zh": "操作接口名，系统规定参数，取值：SetIgnoreQueryStringConfig"
      },
      "required": false,
      "hidden": true
    },
    {
      "name": "OwnerId",
      "position": "Query",
      "type": "Long",
      "description": {
        "zh": ""
      },
      "required": false,
      "hidden": true
    },
    {
      "name": "SecurityToken",
      "position": "Query",
      "type": "String",
      "description": {
        "zh": ""
      },
      "required": false,
      "hidden": false
    },
    {
      "name": "DomainName",
      "position": "Query",
      "type": "String",
      "description": {
        "zh": "要开启该功能的加速域名"
      },
      "required": true,
      "hidden": false
    },
    {
      "name": "Enable",
      "position": "Query",
