{
  "name": "SetVideoSeekConfig",
  "protocol": "HTTP|HTTPS",
  "method": "POST|GET",
  "descriptions": {
    "zh": "\n\n设置拖拽播放功能\n\n注意：\n* 需要源站支持range请求,即对于http请求头中包含 Range 字段,源站能够响应正确的206文件分片\n* 目前支持文件格式有：MP4和FLV\n\n"
  },
  "parameters": [
    {
      "name": "AccessKeyId",
      "position": "Query",
      "type": "String",
      "description": {
        "zh": "当认证方式为Accesskey时必填，用户需要显式指定，校验签名使用，不指定则不校验签名"
      },
      "required": false,
      "hidden": false
    },
    {
      "name": "Action",
      "position": "Query",
      "type": "String",
      "description": {
        "zh": "操作接口名，系统规定参数，取值：SetVideoSeekConfig"
      },
      "required": false,
      "hidden": true
    },
    {
      "name": "OwnerId",
      "position": "Query",
      "type": "Long",
      "description": {
        "zh": ""
      },
      "required": false,
      "hidden": true
    },
    {
      "name": "SecurityToken",
      "position": "Query",
      "type": "String",
      "description": {
        "zh": ""
      },
      "required": false,
      "hidden": false
    },
    {
      "name": "DomainName",
      "position": "Query",
      "type": "String",
      "description": {
        "zh": "加速域名"
      },
      "required": true,
      "hidden": false
    },
    {
      "name": "Enable",
      "position": "Query",
      "type": "String",
      "description": {
        "zh": "配置拖拽播放功能的开启或关闭 取值：On、Off"
      },
      "required": true,
      "hidden": false
    }
  ]
}