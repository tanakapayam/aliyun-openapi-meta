{
  "name": "SetSourceHostConfig",
  "protocol": "HTTP|HTTPS",
  "method": "POST|GET",
  "descriptions": {
    "zh": "\n\n设置回源host功能\n\n注意：\n若源站为OSS域名，需将OSS域名设置为回源host（即源站域名），方可正常回源\n\n"
  },
  "parameters": [
    {
      "name": "AccessKeyId",
      "position": "Query",
      "type": "String",
      "description": {
        "zh": "当认证方式为Accesskey时必填，用户需要显式指定，校验签名使用，不指定则不校验签名"
      },
      "required": false,
      "hidden": false
    },
    {
      "name": "Action",
      "position": "Query",
      "type": "String",
      "description": {
        "zh": "操作接口名，系统规定参数，取值：SetSourceHostConfig"
      },
      "required": false,
      "hidden": true
    },
    {
      "name": "OwnerId",
      "position": "Query",
      "type": "Long",
      "description": {
        "zh": ""
      },
      "required": false,
      "hidden": true
    },
    {
      "name": "SecurityToken",
      "position": "Query",
      "type": "String",
      "description": {
        "zh": ""
      },
      "required": false,
      "hidden": false
    },
    {
      "name": "DomainName",
      "position": "Query",
      "type": "String",
      "description": {
        "zh": "您的加速域名"
      },
      "required": true,
      "hidden": false
    },
    {
      "name": "Enable",
      "position": "Query",
      "type": "String",
      "description": {
        "zh": "枚举on,off。是否打开自定义host配置，默认值为on"
      },
      "required": false,
      "hidden": false
    },
    {
      "name": "BackSrcDomain",
      "position": "Query",
      "type": "String",
      "description": {
        "zh": "自定义回源域名"
      },
      "required": false,
      "hidden": false
    }
  ]
}