{
  "name": "AddCasterComponent",
  "protocol": "HTTP|HTTPS",
  "method": "POST|GET",
  "descriptions": {
    "zh": "添加组件\n\n"
  },
  "parameters": [
    {
      "name": "AccessKeyId",
      "position": "Query",
      "type": "String",
      "description": {
        "zh": "当认证方式为Accesskey时必填，用户需要显式指定，校验签名使用，不指定则不校验签名"
      },
      "required": false,
      "hidden": false
    },
    {
      "name": "Action",
      "position": "Query",
      "type": "String",
      "description": {
        "zh": "系统规定参数，取值：AddCasterComponent"
      },
      "required": false,
      "hidden": true
    },
    {
      "name": "Version",
      "position": "Query",
      "type": "String",
      "description": {
        "zh": "API 版本号为日期形式：YYYY-MM-DD。本版本对应为：2016-11-01。"
      },
      "required": false,
      "hidden": false
    },
    {
      "name": "OwnerId",
      "position": "Query",
      "type": "Long",
      "description": {
        "zh": ""
      },
      "required": false,
      "hidden": true
    },
    {
      "name": "SecurityToken",
      "position": "Query",
      "type": "String",
      "description": {
        "zh": ""
      },
      "required": false,
      "hidden": false
    },
    {
      "name": "CasterId",
      "position": "Query",
      "type": "String",
      "description": {
        "zh": "导播间Id"
      },
      "required": true,
      "hidden": false
    },
    {
      "name": "ComponentName",
      "position": "Query",
      "type": "String",
      "description": {
        "zh": "组件名称，默认为组件ID"
      },
      "required": false,
      "hidden": false
    },
    {
      "name": "LocationId",
      "position": "Query",
      "type": "String",
      "description": {
        "zh": "用于指定组件位置，每个位置至多设置一个组件，格式需符合“RC01…RC12”"
      },
      "required": true,
      "hidden": false
    },
    {
      "name": "ComponentType",
      "position": "Query",
      "type": "String",
      "description": {
        "zh": "组件类型，只能是预设值的一种: “text”/“image”等。"
      },
      "required": true,
      "hidden": false
    },
    {
      "name": "Effect",
      "position": "Query",
      "type": "String",
      "description": {
        "zh": "组件显示的特效，默认为none none-无 animateH-水平滚动 animateV-垂直滚动 FadeIn-渐入 FadeOut-渐出 FlyIn-飞入 FlyOut-飞出"
      },
      "required": false,
      "hidden": false
    },
    {
      "name": "ComponentLayer",
      "position": "Query",
      "type": "String",
      "description": {
        "zh": "设置该组件Layer的尺寸，布局等信息，JSON格式字符串，参数名采用首字母大写、驼峰格式"
      },
      "required": true,
      "hidden": false
    },
    {
      "name": "TextLayerContent",
      "position": "Query",
      "type": "String",
      "description": {
        "zh": "如果是text，设置该Layer元素属性，JSON格式字符串，参数名采用首字母大写，驼峰格式"
      },
      "required": false,
      "hidden": false
    },
    {
      "name": "ImageLayerContent",
      "position": "Query",
      "type": "String",
      "description": {
        "zh": "如果是image，设置该Layer元素属性，JSON格式字符串，参数名采用首字母大写，驼峰格式"
      },
      "required": false,
      "hidden": false
    }
  ]
}